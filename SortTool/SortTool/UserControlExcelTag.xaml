<UserControl x:Class="SortTool.UserControlExcelTag"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SortTool"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="200"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="LabelName"  Grid.Column="0" Content="Label"  VerticalAlignment="Center" Width="100"  HorizontalAlignment="Center"/>
        <ComboBox x:Name="LabelSelect" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Width="141" />
    </Grid>
</UserControl>
