<Window x:Class="UI_Design.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI_Design"
        mc:Ignorable="d"
        Title="Translation Helper" Height="200" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="200"/>
        </Grid.ColumnDefinitions>
        <Button Content="Similarity Part" Grid.Column="0" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="200" Height="169" Click="SimilarityWindow"/>
        <Button Content="Merge Part" Grid.Column="1" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="192" Height="169" Click="MergeWindow"/>
    </Grid>
</Window>
